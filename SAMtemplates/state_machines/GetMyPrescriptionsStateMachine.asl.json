{
  "Comment": "Get My Prescriptions State Machine",
  "StartAt": "Get My Prescriptions",
  "States": {
    "Get My Prescriptions": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
      "Parameters": {
        "Payload.$": "$",
        "FunctionName": "${GetMyPrescriptionsFunctionArn}"
      },
      "Next": "Get My Prescriptions Result"
    },
    "Get My Prescriptions Result": {
      "Type": "Choice",
      "Choices": [
        {
          "Not": {
            "Variable": "$.Payload.statusCode",
            "NumericEquals": 200
          },
          "Next": "Failed Get My Prescriptions"
        }
      ],
      "Default": "Get Status Updates"
    },
    "Failed Get My Prescriptions": {
      "Type": "Pass",
      "End": true
    },
    "Get Status Updates": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
      "OutputPath": "$.Payload",
      "Parameters": {
        "Payload.$": "$",
        "FunctionName": "arn:aws:lambda:eu-west-2:591291862413:function:psu-ab-1-GSUL:$LATEST"
      },
      "End": true
    }
  }
}
